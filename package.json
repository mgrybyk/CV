{
  "name": "cv",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite-ssg build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test-screenshots": "playwright test --config playwright.screenshots.config.ts",
    "test-lighthouse": "playwright test --config playwright.lighthouse.config.ts"
  },
  "dependencies": {
    "@unocss/reset": "^0.60.2",
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@babel/types": "^7.24.5",
    "@playwright/test": "^1.44.0",
    "@rushstack/eslint-patch": "^1.10.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.12",
    "@unocss/eslint-config": "^0.60.2",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "lighthouse": "^12.0.0",
    "lighthouse-reporting": "^1.6.7",
    "npm-run-all": "^4.1.5",
    "playwright-lighthouse": "^4.0.0",
    "prettier": "^3.2.5",
    "typescript": "~5.4.5",
    "unocss": "^0.60.2",
    "vite": "^5.2.11",
    "vite-ssg": "^0.23.7",
    "vue-tsc": "^2.0.17"
  }
}
